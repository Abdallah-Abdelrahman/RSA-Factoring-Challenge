#!/usr/bin/python3

"""This module represents the RSA challenge."""

from sys import argv
from math import isqrt


if len(argv) != 2:
    exit(1)

with open(argv[1], 'r') as file:
    while line := file.readline():
        if (len(line) > 15):
            continue
        is_prime = False
        prime = 2
        num = int(line)
        while prime <= isqrt(num):
            if (num % prime == 0):
                is_prime = False
                print(f'{num}={num // prime}*{prime}')
                break
            prime += 1
            is_prime = True
        if is_prime:
            print(f'{num}={num}*1')
# no condition => 31G
# > 15 => 34G
# > 17 => 34G
# > 20 => 29G
# isqrt => 29G
