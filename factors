#!/usr/bin/python3

"""This module represents the RSA challenge."""

from sys import argv


if len(argv) != 2:
    exit(1)

with open(argv[1], 'r') as file:
    while line := file.readline():
        prime = 2
        num = int(line)
        while num % prime != 0:
            prime += 1
        print(f'{num}={num // prime}*{prime}')
# no condition => 30G
# > 15 => 34G
# > 20 => 29G
